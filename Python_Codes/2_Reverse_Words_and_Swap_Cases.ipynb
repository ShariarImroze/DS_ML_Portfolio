    "def reverse_words_order_and_swap_cases(sentence):\n",
    "    words = sentence.split()\n",
    "    \n",
    "    reversed = ' '.join([word.swapcase() for word in words[::-1]])\n",
    "    \n",
    "    return reversed "
   ]
